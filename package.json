{
	"name": "wplace darkmode",
	"version": "1.1.0",
	"description": "a dark theme for wplace.live",
	"module": "src/index.ts",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/z1xus/wplace-darkmode.git"
	},
	"bugs": {
		"url": "https://github.com/z1xus/wplace-darkmode/issues"
	},
	"homepage": "https://github.com/z1xus/wplace-darkmode#readme",
	"userscriptHeader": {
		"@match": [
			"https://wplace.live/*",
			"https://*.wplace.live/*"
		],
		"@grant": [
			"GM_addStyle",
			"GM_getValue",
			"GM_setValue",
			"GM_registerMenuCommand"
		],
		"@run-at": "document-start",
		"@noframes": "true"
	},
	"author": "z1xus",
	"license": "MIT",
	"devDependencies": {
		"@types/bun": "latest",
		"@types/yargs": "^17.0.33",
		"bun-style-loader": "^0.4.0",
		"prettier": "^3.6.2",
		"rimraf": "^6.0.1",
		"sass": "^1.79.4",
		"type-fest": "^4.26.1",
		"winston": "^3.14.2",
		"yargs": "^17.7.2"
	},
	"peerDependencies": {
		"typescript": "^5.6.2"
	},
	"scripts": {
		"build": "bun build.ts",
		"build:dev": "bun build.ts --dev",
		"build:watch": "bun build.ts --dev --watch",
		"build:workflow": "bun build.ts --release-channel GitHubRelease",
		"clean": "rimraf ./dist/",
		"dev": "bun build.ts --dev --server --watch",
		"format": "prettier --write ."
	},
	"dependencies": {},
	"trustedDependencies": [
		"@parcel/watcher"
	]
}
